				<artifactId>spring-boot-maven-plugin</artifactId>
				<configuration>
					<!-- springboot main方法入口 -->
					<mainClass>com.jsy.community.LeaseServiceApp</mainClass>
					<layout>ZIP</layout>
				</configuration>
				<executions>
					<execution>
