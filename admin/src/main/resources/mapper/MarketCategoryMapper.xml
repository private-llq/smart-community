<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.jsy.community.mapper.MarketCategoryMapper">
    <update id="updateSort">
        update t_proprietor_market_category set sort = sort + 1 where sort &gt;= #{sort}
    </update>
</mapper>