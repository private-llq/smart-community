<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jsy.community.mapper.AppMenuMapper">
    <select id="getMenuIdByCommunityId" resultType="long">
        SELECT menu_id
        FROM t_menu_community
        where community_id = #{communityId}
    </select>

    <insert id="addParentMenu">
        insert into t_menu_community (community_id,menu_id)values (#{communityId},#{id})
    </insert>
</mapper>
